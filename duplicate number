import java.util.stream.*;
import java.util.*;
import java.util.function.*;


public class Main {
    public static void main(String[] args) {
        List<Integer> numbers = Arrays.asList(1, 2, 3, 2, 4, 5, 1, 6, 3, 7);

             List<Integer> duplicates= numbers.stream().collect(Collectors.groupingBy(Function.identity(),Collectors.counting()))
                  .entrySet().stream().filter(val->val.getValue()>1).map(Map.Entry::getKey)
                     .toList();

        System.out.println("Duplicates: " + duplicates);
    }

}
